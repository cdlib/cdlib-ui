<div class="c-filter">
  <div class="c-filter__group">
    <div class="c-filter__label" id="c-filter-show-label">Show:</div>
    <ul id="js-filter__show-group" aria-labelledby="c-filter-show-label">
      {{#each buttonList1}}
      <li><a href="#">{{ button.text }}</a></li>
      {{/each}}
    </ul>
  </div>
  <div class="c-filter__group">
    <div class="c-filter__label" id="c-filter-sort-label">Sort by:</div>
    <ul id="js-filter__sort-group" aria-labelledby="c-filter-sort-label">
      {{#each buttonList2}}
      <li><a href="#">{{ button.text }}</a></li>
      {{/each}}
    </ul>
  </div>
</div>
