<aside aria-label="last updated by page owner" class="c-updated">
  <div id="updated-last" class="c-updated__label">Last updated:</div>
  <time aria-labelledby="updated-last">{{ updated.date }}</time>
  <div id="updated-owner" class="c-updated__label">Page owner:</div>
  <address aria-labelledby="updated-owner">
    {{> @link }}
  </address>
</aside>
