<nav class="c-secondarynav o-sidebarnav" v-onmedia.screen2="detectMediaQuery">
  <header>
    {{> @heading }}
    <template v-if="!$mq.screen2">
      <button class="o-button5" v-on:click="toggle1" v-if="isDisplayed">{{ secondarynav.closeButtonText }}</button>
      <button class="o-button6" v-on:click="toggle1" v-if="!isDisplayed">{{ secondarynav.openButtonText }}</button>
    </template>
  </header>
  <div v-if="isDisplayed">
    <ul>
      <li>{{> @link--1 }}
        <ul>
          {{#each list1}}
          <li>{{> @link--1 }}</li>
          {{/each}}
          <li class="current_page_item">{{> @link--1 }}
            <ul>
              {{#each list2}}
              <li>{{> @link--1 }}</li>
              {{/each}}
              <li class="current_page_item">{{> @link--1 }}</li>
            </ul>
          </li>
        </ul>
      </li>
    </ul>
  </div>
</nav>
