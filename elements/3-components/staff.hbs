<table class="c-staff">
  <thead>
    <tr>
      <th>Name</th>
      <th>Title</th>
      <th>Program</th>
      <th>Contact</th>
    </tr>
  </thead>
  <tbody>
    {{#each row}}
    <tr>
      <th scope="row">{{ lastname.text }}, {{ firstname.text }}</th>
      <td>{{ title.text }}</td>
      <td>{{ program.text }}</td>
      <td>
        <a href="">
          <span>View profile</span>
        </a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>
