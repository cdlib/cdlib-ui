<table class="c-staff">
  <thead>
    <tr>
      <th>Name</th>
      <th>Title</th>
      <th>Contact</th>
    </tr>
  </thead>
  <tbody>
    {{#each row}}
    <tr>
      <th scope="row">{{ lastname.text }}, {{ firstname.text }}</th>
      <td>{{ position.text }}</td>
      <td>
        <a href="">
          <span>View profile</span>
        </a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>
