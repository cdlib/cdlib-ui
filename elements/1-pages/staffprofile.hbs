<div class="t-layout--3">
  {{ render '@skipnav' }}
  {{ render '@header' }}
  {{ render '@breadcrumb' }}
  <aside aria-label="first" class="t-layout__sidebar1">
    {{ render '@secondarynav' }}
  </aside>
  <main id="maincontent">
    {{> @heading--1 }}
    {{> @contact }}
    {{ render '@avatar' }}
    <h2>{{ program.heading }}</h2>
    <p>{{ program.text }}</p>
    <h2>{{ title.heading }}</h2>
    <p>{{ title.text }}</p>
    <h2>{{ responsibilities.heading }}</h2>
    <p>{{ responsibilities.text }}</p>
    <h2>{{ education.heading }}</h2>
    <p>{{ education.text }}</p>
    <h2>{{ background.heading }}</h2>
    <p>{{ background.text }}</p>
    <h2>{{ interests.heading }}</h2>
    <p>{{ interests.text }}</p>
    <h2>{{ affiliations.heading }}</h2>
    <p>{{ affiliations.text }}</p>
    <h2><abbr title="Orchid">ORCID</abbr> ID</h2>
    {{ render '@orcid' }}
    <h2>{{ social.heading }}</h2>
    <a href="{{ social.url }}">{{ social.url }}</a>
  </main>
  <aside aria-label="second" class="t-layout__sidebar2">
    {{ render '@news' }}
  </aside>
  {{ render '@updated' }}
  {{ render '@footer' }}
</div>
